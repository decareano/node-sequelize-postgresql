        <form action="/add-task" id="addTaskForm" method="post" role="form">


            <label>Business Unit:</label>
            <input type="text" class="form-control" name="business_unit" placeholder="Enter your unit name"><br>
            <label>Lab Name:</label>
            <input type="text" class="form-control" name="lab_name" placeholder="Enter your lab name"><br>
            <label>Location:</label>
            <input type="text" class="form-control" name="location" placeholder="Enter a location"><br>
            <label>Validated:</label>
            <input type="validated" class="form-control" name="validated" placeholder="Yes or No"><br>
            <label>Computer Name:</label>
            <input type="computer_name" class="form-control" name="computer_name" placeholder="Enter a computer name"><br>
	    <label>BMRN Asset:</label>
            <input type="numeric" class="form-control" name="bmrn_asset" placeholder="Enter an asset number here"><br>
	    <label>PC Make Model:</label>
            <input type="pc_make_model" class="form-control" name="pc_make_model" placeholder="Enter pc model"><br>

	    <label>OS:</label>
            <input type="os" class="form-control" name="os" placeholder="Enter an OS for your PC"><br>

            
                  <label>Notes:</label>
                  <input type="notes" class="form-control" name="notes" placeholder="Enter a short description"><br>

            
                  <label>System Owner:</label>
                  <input type="system_owner" class="form-control" name="system_owner" placeholder="Enter name of person who owns the instrument"><br>
            
		
                  <label>Vendor:</label>
                  <input type="vendor" class="form-control" name="vendor" placeholder="Enter name of Instrument Vendor"><br>
            
                  <label>Model Instrument Name:</label>
                  <input type="model_instrument_name" class="form-control" name="model_instrument_name" placeholder="Enter a name for your instrument model"><br>
            
                  <label>Instrument Friendly Name:</label>
                  <input type="instrument_friendly_name" class="form-control" name="instrument_friendly_name" placeholder="Enter a short name for instrument"><br>
            
                  <label>Instrument Validation ID:</label>
                  <input type="instrument_validation_id" class="form-control" name="instrument_validation_id" placeholder="Enter a validation ID if it exists"><br>
            
                  <label>Instrument Software:</label>
                  <input type="instrument_software" class="form-control" name="instrument_software" placeholder="Enter name of software"><br>
            
                  <label>Backup Data Format:</label>
                  <input type="backup_data_format" class="form-control" name="backup_data_format" placeholder="Enter backup format"><br>
         
		
                  <label>Nic Address:</label>
                  <input type="nic_address" class="form-control" name="nic_address" placeholder="Enter a NIC address for your instrument"><br>
            
                  <label>Ethernet Jack:</label>
                  <input type="ethernet_jack" class="form-control" name="ethernet_jack" placeholder="Enter a jack number"><br>
            
                  <label>IP Address VLAN:</label>
                  <input type="ip_address_vlan" class="form-control" name="ip_address_vlan" placeholder="Enter a VLAN"><br>
            
                  <label>Image:</label>
                  <input type="image" class="form-control" name="image" placeholder="Enter an Image backup"><br>
            
                  <label>Backup:</label>
                  <input type="backup" class="form-control" name="backup" placeholder="Enter a location for the backup"><br>
           
                  <label>Decommisioned:</label>
                  <input type="decommisioned" class="form-control" name="decommisioned" placeholder="Enter note when system is decommisioned"><br>
            
                  <label>Last Verified:</label>
                  <input type="last_verified" class="form-control" name="last_verified" placeholder="Enter a name of the person who verified the data entry"><br>
           
                  <label>Add a Screenshot:</label>
                  <input type="Add a Screenshot" name="Add a Screenshot" id="profilepic"><br>
            <div>
	<input type="submit" class="btn btn-primary" value="Submit"/>
  <div class="row">
  
  <div class="col-sm-4">
  <label>Add a Screenshot:</label>
                  <input type="Add a Screenshot" name="Add a Screenshot" id="profilepic">
